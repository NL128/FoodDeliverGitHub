@model  bool? 
<div class="popUp-food-default" id="confirmationI">
    <div class="centerWindow">
        <div class="setMaxWidth">
            <div class="left-Right">
                <div id="map" class="showMap"></div>
            </div>
            <div class="left-Right">
                <div class="setMaxWidth flexRight">
                    <div class="xButton" onclick="closeWindow()">
                        <img src="data:image/jpeg;base64," />
                    </div>
                </div>
                <div class="setMaxWidth">
                    <h4>Potvrda adrese</h4>


                </div>
                <div class="setMaxWidth">
                    <p>Potvrdite da je vasa adresa ispravno uneta</p>
                </div>
                <div class="setMaxWidth">
                    <div class="centerWindow changeMargin">
                        <label class="inner-lable">Opština</label>
                        <input class="textBox-food" placeholder="Vaša opština" name="opstina" id="municipalityConfirmId" />
                    </div>
                </div>
                <div class="setMaxWidth">
                    <div class="centerWindow changeMargin">
                        <label class="inner-lable">Adresa</label>
                        <input class="textBox-food" placeholder="Vaša adresa" name="adresa" id="addressConfirmId" />
                    </div>
                </div>
                <div class="setMaxWidth">
                    <div class="centerWindow changeMargin">
                        <label class="inner-lable">Broj kuće</label>
                        <input class="textBox-food" placeholder="Vaš kućni broj"  id="houseNumber" />
                    </div>
                </div>
                <div class="setMaxWidth">
                    <div class="centerWindow changeMargin">

                        <label class="inner-lable">Poštanski broj</label>
                        <input class="textBox-food" placeholder="Poštanski broj" name="postCode" id="postalCodeId" />

                    </div>
                </div>
                <div class="setMaxWidth">
                    <div class="centerWindow">
                        @if (Model != null && Model == true)
                        {
                            using (Html.BeginForm("Index", "Ponuda", FormMethod.Post, new { id = "ponudaForm" }))
                            {
                                @Html.AntiForgeryToken()
                                
                                <input type="submit" class="order-btn" value="Potvrdi" />
                            }
                        }else
                        {
                        <button type="button" class="order-btn" onclick="confirmInputValues()">Potvrdi</button>
                        }
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
