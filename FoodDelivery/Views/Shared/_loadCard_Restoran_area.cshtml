@model  DomainLayer.RestoranDomainModel
    @if (Model != null)
    {
<div class="setLargeWidth">

    <div class="store-card">
        <div class="card-image-area">
            <div>
                <img src="data:image/jpeg;base64," />
            </div>
        </div>
        <div class="right-holder">
            <div class="row-card">
                <div class="col-card"><p class="header-title">@Model.Ime</p></div>
                <div class="col-card"><p class="header-title">Vreme <br /> Dostave</p></div>
                <div class="col-card"><p class="header-title">Minimalna <br />Narudzbina</p></div>

            </div>
            <div class="row-card">
                <div class="col-card"><p class="normal-font-card">Mladost 610m.</p></div>
                <div class="col-card"><p class="normal-font-card">30`</p></div>
                <div class="col-card"><p class="normal-font-card">@Model.MinimalnaNarudzbina RSD</p></div>
            </div>
            <div class="row-card">
                <div class="col-card"><p class="normal-font-card"></p></div>
                <div class="col-card"><p class="normal-font-card">@Model.Kuhinja</p></div>
              
            </div>
            <div class="row-card">
                <div class="col-card"><p class="normal-font-card">Ocena @Model.Ocena  </p></div>
                <div class="col-card">
                   @if (Model.DostavaSeNaplacuje == true)
                   {
                    <p class="normal-font-card">
                        Doplata za dostavu
                    </p>
                   }
                   else
                   {
                       <p class="normal-font-card">
                        Besplatna Dostava
                    </p>
                   }


                 </div>
   
                   <div class="col-card"></div>
            </div>
        </div>
        
       
                
                
     </div>
</div>
    }
